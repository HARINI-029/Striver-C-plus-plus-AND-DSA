# include <iostream>
# include <list>
using namespace std;

int main ()
{
    list <int> l;
    l.push_back (10);// {10}
    l.emplace_back (20);//{10,20}
    l.push_front (5); //{5,10,20}
    l.emplace_front (0);//{0,5,10,20}
    
    // few functions same as vector:

    
    for (auto i = l.begin (); i!=l.end();i++)
    {
        cout<<*i<<" "<<endl;// cannot use indexing
    }
    
    cout<<l.size()<<endl;
  //  l.insert (l.begin()+ 1,2);//{0,2,5,10,20}// CANNOT USE THIS METHOD
  
  auto i = l.begin ();
  advance (i,1);
  l.insert (i,2);// {0,2,5,10,20}
    l.clear();
    list <int> l2 = {2};
    l.push_back (3);
    l.swap(l2); // l2= 3 and l= 2
    
    
}
